<%= form_for @school, :html => { :multipart => true, :class => '',:autocomplete => 'off'} do |f| %>
<div class="form-group">
	<div class="col-sm-12">
		<%= f.text_field :name, :class => "form-control",
		:placeholder => "School Name"%>
	</div>
</div></br></br></br>
<div class="form-group">
	<div class="col-sm-12">
		<%= f.text_field :type, :class => "form-control",
		:placeholder => "Type"%>
	</div>
</div></br></br></br></br>
<div class="form-group">
	<div class="col-sm-12">
		<%= f.text_area :address, :class => "form-control",
		:placeholder => "Adress"%>
	</div>
</div>
<div class="creat_village_btn create_btn_position_fix">
	<button class="btn btn-primary" type="submit">
		<span class=""></span>&nbsp;&nbsp;
		Create School
	</button>
</div>
<% end %>